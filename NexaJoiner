-- Functions
local function updateStatus(text)
    statusLabel.Text = "Status: " .. text
    print("[Nexa] " .. text)
end

local function updateBackgroundStatus(text, color)
    bgStatusLabel.Text = text
    bgStatusLabel.TextColor3 = color or Color3.fromRGB(100, 255, 100)
end

local function updateBackgroundJob(name, money)
    bgCurrentJobLabel.Text = name .. " - " .. money
end

local function parseMoneyPerSec(moneyText)
    if not moneyText then return 0 end
    local amount, multiplier = moneyText:match("%$?([%d%.]+)([MKmkBb]?)")
    if not amount then return 0 end
    
    local value = tonumber(amount) or 0
    multiplier = (multiplier or ""):upper()
    
    if multiplier == "M" then
        value = value * 1000000
    elseif multiplier == "K" then
        value = value * 1000
    elseif multiplier == "B" then
        value = value * 1000000000
    end
    
    return value
end

local function createJobEntry(jobName, money, jobId, players)
    local entry = Instance.new("Frame")
    entry.Size = UDim2.new(1, -10, 0, 70)
    entry.BackgroundColor3 = Color3.fromRGB(45, 55, 45)
    entry.BorderSizePixel = 0
    entry.Parent = jobListFrame
    
    local entryCorner = Instance.new("UICorner")
    entryCorner.CornerRadius = UDim.new(0, 6)
    entryCorner.Parent = entry
    
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Size = UDim2.new(1, -20, 0, 20)
    nameLabel.Position = UDim2.new(0, 10, 0, 5)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Text = jobName
    nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.TextSize = 13
    nameLabel.TextXAlignment = Enum.TextXAlignment.Left
    nameLabel.TextTruncate = Enum.TextTruncate.AtEnd
    nameLabel.Parent = entry
    
    local moneyLabel = Instance.new("TextLabel")
    moneyLabel.Size = UDim2.new(0.5, -10, 0, 18)
    moneyLabel.Position = UDim2.new(0, 10, 0, 28)
    moneyLabel.BackgroundTransparency = 1
    moneyLabel.Text = "ğŸ’° " .. money
    moneyLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
    moneyLabel.Font = Enum.Font.GothamBold
    moneyLabel.TextSize = 12
    moneyLabel.TextXAlignment = Enum.TextXAlignment.Left
    moneyLabel.Parent = entry
    
    local playersLabel = Instance.new("TextLabel")
    playersLabel.Size = UDim2.new(0.5, -10, 0, 18)
    playersLabel.Position = UDim2.new(0.5, 0, 0, 28)
    playersLabel.BackgroundTransparency = 1
    playersLabel.Text = "ğŸ‘¥ " .. players .. "/8"
    playersLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
    playersLabel.Font = Enum.Font.Gotham
    playersLabel.TextSize = 11
    playersLabel.TextXAlignment = Enum.TextXAlignment.Right
    playersLabel.Parent = entry
    
    local jobIdLabel = Instance.new("TextLabel")
    jobIdLabel.Size = UDim2.new(1, -20, 0, 16)
    jobIdLabel.Position = UDim2.new(0, 10, 0, 48)
    jobIdLabel.BackgroundTransparency = 1
    jobIdLabel.Text = "ğŸ†” " .. jobId:sub(1, 30) .. "..."
    jobIdLabel.TextColor3 = Color3.fromRGB(100, 200, 255)
    jobIdLabel.Font = Enum.Font.Code
    jobIdLabel.TextSize = 8
    jobIdLabel.TextXAlignment = Enum.TextXAlignment.Left
    jobIdLabel.TextTruncate = Enum.TextTruncate.AtEnd
    jobIdLabel.Parent = entry
    
    return entry
end

local function teleportToJob(jobId, jobName, jobMoney)
    if isJoining then
        updateStatus("Already joining...")
        return
    end
    
    processedJobs[jobId] = true
    isJoining = true
    
    updateStatus("ğŸ”„ Joining: " .. jobName)
    
    if currentMode == "background" then
        updateBackgroundStatus("ğŸ”„ Joining...", Color3.fromRGB(255, 200, 100))
        updateBackgroundJob(jobName, jobMoney)
    end
    
    print("[Nexa] Teleporting to Job ID: " .. jobId)
    print("[Nexa] Job: " .. jobName .. " - " .. jobMoney)
    
    wait(0.5)
    
    local success, errorMsg = pcall(function()
        TeleportService:TeleportToPlaceInstance(GAME_ID, jobId, player)
    end)
    
    if not success then
        updateStatus("âŒ Teleport failed")
        print("[Nexa] Error:", errorMsg)
        
        if currentMode == "background" then
            updateBackgroundStatus("âŒ Failed", Color3.fromRGB(255, 100, 100))
        end
        
        wait(3)
        isJoining = false
        
        if currentMode == "background" then
            updateBackgroundStatus("ğŸŸ¢ Active", Color3.fromRGB(100, 255, 100))
            updateBackgroundJob("Waiting...", "")
        end
    end
end

local function clearJobList()
    for _, child in pairs(jobListFrame:GetChildren()) do
        if child:IsA("Frame") then
            child:Destroy()
        end
    end
end

local function addJobLog(jobData)
    for i, log in pairs(jobLogs) do
        if log.jobId == jobData.jobId then
            jobLogs[i] = jobData
            clearJobList()
            for _, log in pairs(jobLogs) do
                createJobEntry(log.name, log.money, log.jobId, log.players)
            end
            jobListFrame.CanvasSize = UDim2.new(0, 0, 0, listLayout.AbsoluteContentSize.Y + 10)
            return
        end
    end
    
    table.insert(jobLogs, 1, jobData)
    createJobEntry(jobData.name, jobData.money, jobData.jobId, jobData.players)
    jobListFrame.CanvasSize = UDim2.new(0, 0, 0, listLayout.AbsoluteContentSize.Y + 10)
end

local function readJobsFromFile()
    if not canReadFiles then
        infoLabel.Text = "âŒ Executor Not Supported!"
        infoLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        updateStatus("âŒ Your executor doesn't support file operations!")
        return
    end
    
    spawn(function()
        local firstRead = true
        
        while wait(CHECK_INTERVAL) do
            local success, result = pcall(function()
                local fileContent = readfile(JOBS_FILE)
                local jobs = game:GetService("HttpService"):JSONDecode(fileContent)
                
                if firstRead then
                    firstRead = false
                    print("[Nexa] âœ… Successfully reading jobs file!")
                end
                
                if jobs and #jobs > 0 then
                    infoLabel.Text = "ğŸ“ File: Active (" .. #jobs .. " jobs)"
                    infoLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
                    
                    if currentMode == "background" then
                        updateBackgroundStatus("ğŸ“¡ " .. #jobs .. " jobs", Color3.fromRGB(100, 255, 100))
                    end
                    
                    local bestJob = nil
                    local bestMoney = 0
                    
                    for _, job in ipairs(jobs) do
                        if job.name and job.jobId and job.money then
                            local money = parseMoneyPerSec(job.money)
                            
                            addJobLog({
                                name = job.name,
                                money = job.money,
                                jobId = job.jobId,
                                players = job.players or "0"
                            })
                            
                            if money > bestMoney and not processedJobs[job.jobId] then
                                bestMoney = money
                                bestJob = job
                            end
                        end
                    end
                    
                    -- Auto-join
                    if (currentMode == "auto" or currentMode == "background") and bestJob and not isJoining then
                        print("[Nexa] Best job found: " .. bestJob.name)
                        spawn(function()
                            wait(0.5)
                            teleportToJob(bestJob.jobId, bestJob.name, bestJob.money)
                        end)
                    end
                else
                    infoLabel.Text = "ğŸ“ File: Active (0 jobs)"
                    infoLabel.TextColor3 = Color3.fromRGB(255, 200, 100)
                    
                    if currentMode == "background" then
                        updateBackgroundStatus("â¸ï¸ No jobs", Color3.fromRGB(255, 200, 100))
                    end
                end
            end)
            
            if not success then
                infoLabel.Text = "âŒ Cannot read file!"
                infoLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
                
                if firstRead then
                    print("[Nexa] Cannot read file: " .. JOBS_FILE)
                    print("[Nexa] Error: " .. tostring(result))
                    print("[Nexa] Make sure Python app is saving to the correct location!")
                end
                
                if currentMode == "background" then
                    updateBackgroundStatus("ğŸ”´ No File", Color3.fromRGB(255, 100, 100))
                end
            end
        end
    end)
end

-- Button Handlers
autoButton.MouseButton1Click:Connect(function()
    if currentMode == "auto" then
        currentMode = "manual"
        autoButton.BackgroundColor3 = Color3.fromRGB(40, 180, 80)
        autoButton.Text = "ğŸ¯ AUTO JOIN (OFF)"
        bgStatusFrame.Visible = false
        mainFrame.Visible = true
        updateStatus("â¹ï¸ Auto OFF")
        return
    end
    
    currentMode = "auto"
    autoButton.BackgroundColor3 = Color3.fromRGB(60, 100, 60)
    autoButton.Text = "ğŸ¯ AUTO JOIN (ON)"
    backgroundButton.BackgroundColor3 = Color3.fromRGB(80, 40, 180)
    bgStatusFrame.Visible = false
    mainFrame.Visible = true
    updateStatus("ğŸŸ¢ Auto ON - Will join best job!")
end)

backgroundButton.MouseButton1Click:Connect(function()
    if currentMode == "background" then
        currentMode = "manual"
        backgroundButton.BackgroundColor3 = Color3.fromRGB(80, 40, 180)
        bgStatusFrame.Visible = false
        mainFrame.Visible = true
        updateStatus("â¹ï¸ Background OFF")
        return
    end
    
    currentMode = "background"
    backgroundButton.BackgroundColor3 = Color3.fromRGB(120, 60, 200)
    autoButton.BackgroundColor3 = Color3.fromRGB(40, 180, 80)
    autoButton.Text = "ğŸ¯ AUTO JOIN (OFF)"
    bgStatusFrame.Visible = true
    mainFrame.Visible = false
    updateBackgroundStatus("ğŸŸ¢ Active", Color3.fromRGB(100, 255, 100))
    updateBackgroundJob("Waiting...", "")
    updateStatus("ğŸ‘» Background Mode Active")
end)

stopButton.MouseButton1Click:Connect(function()
    currentMode = "manual"
    autoButton.BackgroundColor3 = Color3.fromRGB(40, 180, 80)
    autoButton.Text = "ğŸ¯ AUTO JOIN (OFF)"
    backgroundButton.BackgroundColor3 = Color3.fromRGB(80, 40, 180)
    bgStatusFrame.Visible = false
    mainFrame.Visible = true
    updateStatus("â¹ï¸ Stopped")
end)

-- Keep background in position
RunService.Heartbeat:Connect(function()
    if bgStatusFrame.Visible and bgStatusFrame.Position ~= UDim2.new(1, -230, 1, -70) then
        bgStatusFrame.Position = UDim2.new(1, -230, 1, -70)
    end
end)

-- Start
readJobsFromFile()

print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
