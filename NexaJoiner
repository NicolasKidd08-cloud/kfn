-- CHILLI HUB FINDER - Shows EXACT GUI structure
-- This will tell us the real name of Chilli Hub

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

print("\n\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("ğŸ” CHILLI HUB DIAGNOSTIC TOOL")
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\n")

-- List ALL ScreenGuis in PlayerGui
print("ğŸ“‹ ALL SCREEN GUIS IN PLAYERGUI:")
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")

for _, gui in pairs(playerGui:GetChildren()) do
	if gui:IsA("ScreenGui") then
		print("\nâœ… GUI Name:", gui.Name)
		print("   Enabled:", gui.Enabled)
		
		-- Check if it has Chilli-related text
		local hasChilli = false
		for _, obj in pairs(gui:GetDescendants()) do
			if obj:IsA("TextLabel") or obj:IsA("TextButton") then
				local text = obj.Text or ""
				if text:lower():find("chilli") or text:find("Server Tools") then
					hasChilli = true
					print("   â­ CONTAINS CHILLI TEXT:", text)
					break
				end
			end
		end
		
		if hasChilli then
			print("\n   ğŸ¯ THIS IS PROBABLY CHILLI HUB!")
			print("   Searching for textboxes...")
			
			-- Find all textboxes
			for _, obj in pairs(gui:GetDescendants()) do
				if obj:IsA("TextBox") then
					print("\n   ğŸ“¦ FOUND TEXTBOX:")
					print("      Full Path:", obj:GetFullName())
					print("      Name:", obj.Name)
					print("      Placeholder:", obj.PlaceholderText or "none")
					print("      Text:", obj.Text or "empty")
					print("      Visible:", obj.Visible)
					print("      Parent:", obj.Parent and obj.Parent.Name or "none")
				end
			end
			
			-- Find all buttons near "Server Tools"
			print("\n   ğŸ” Looking for buttons near 'Server Tools'...")
			for _, obj in pairs(gui:GetDescendants()) do
				if obj:IsA("TextButton") then
					local text = obj.Text or ""
					if text:find("Join") or text:find("Copy") or text:find("Rejoin") then
						print("\n   ğŸ”˜ FOUND BUTTON:", text)
						print("      Full Path:", obj:GetFullName())
						print("      Name:", obj.Name)
					end
				end
			end
		end
	end
end

print("\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("âœ… DIAGNOSTIC COMPLETE!")
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("\nğŸ’¡ COPY THE EXACT GUI NAME AND TEXTBOX PATH")
print("ğŸ“‹ Then tell me what you found!\n")
