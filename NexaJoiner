    if playerGui:FindFirstChild("NexaJoiner") then 
        playerGui.NexaJoiner:Destroy() 
    end
    if playerGui:FindFirstChild("BackgroundStatus") then 
        playerGui.BackgroundStatus:Destroy() 
    end
end)

