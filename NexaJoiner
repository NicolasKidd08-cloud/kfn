    print("âœ… File operations supported")
else
    print("âŒ File operations NOT supported - won't work!")
end
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
