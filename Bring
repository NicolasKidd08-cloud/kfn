local hh,hs=cpl(plhc,"üòà HUMP",Color3.fromRGB(50,255,120),"H")local bh,bs=cpl(plbc,"üéØ BRING",Color3.fromRGB(255,100,255),"B")

local function cpb(tp,sf,lt,bc)local bt=lt=="H"and plh or plb;if bt[tp]then return bt[tp]end;local b=Instance.new("TextButton")b.Name=tp.Name;b.Size=UDim2.new(1,-5,0,48)b.BackgroundColor3=Color3.fromRGB(22,22,22)b.BorderSizePixel=0;b.Text=""b.AutoButtonColor=false;b.Parent=sf;local co=Instance.new("UICorner")co.CornerRadius=UDim.new(0,8)co.Parent=b;local bs=Instance.new("UIStroke")bs.Color=Color3.fromRGB(40,40,40)bs.Thickness=1;bs.Parent=b;local pi=Instance.new("ImageLabel")pi.Size=UDim2.new(0,38,0,38)pi.Position=UDim2.new(0,5,0,5)pi.BackgroundColor3=Color3.fromRGB(50,50,50)pi.BorderSizePixel=0;pi.Image="https://www.roblox.com/headshot-thumbnail/image?userId="..tp.UserId.."&width=150&height=150&format=png"pi.Parent=b;local pc=Instance.new("UICorner")pc.CornerRadius=UDim.new(0,8)pc.Parent=pi;local un=Instance.new("TextLabel")un.Size=UDim2.new(1,-50,1,0)un.Position=UDim2.new(0,48,0,0)un.BackgroundTransparency=1;un.Text=tp.Name;un.TextSize=13;un.TextColor3=Color3.fromRGB(220,220,220)un.Font=Enum.Font.GothamBold;un.TextXAlignment=Enum.TextXAlignment.Left;un.TextTruncate=Enum.TextTruncate.AtEnd;un.Parent=b;b.MouseEnter:Connect(function()T:Create(b,TweenInfo.new(0.15),{BackgroundColor3=Color3.fromRGB(32,32,32)}):Play()T:Create(bs,TweenInfo.new(0.15),{Color=bc}):Play()end)b.MouseLeave:Connect(function()local sp=lt=="H"and sph or spb;if sp~=tp then T:Create(b,TweenInfo.new(0.15),{BackgroundColor3=Color3.fromRGB(22,22,22)}):Play()T:Create(bs,TweenInfo.new(0.15),{Color=Color3.fromRGB(40,40,40)}):Play()end end)b.MouseButton1Click:Connect(function()local sp=lt=="H"and sph or spb;if sp and bt[sp]then local pb=bt[sp]T:Create(pb,TweenInfo.new(0.15),{BackgroundColor3=Color3.fromRGB(22,22,22)}):Play()local ps=pb:FindFirstChildOfClass("UIStroke")if ps then T:Create(ps,TweenInfo.new(0.15),{Color=Color3.fromRGB(40,40,40)}):Play()end;local pu=pb:FindFirstChild("TextLabel")if pu then T:Create(pu,TweenInfo.new(0.15),{TextColor3=Color3.fromRGB(220,220,220)}):Play()end end;if lt=="H"then sph=tp else spb=tp end;T:Create(b,TweenInfo.new(0.15),{BackgroundColor3=bc}):Play()T:Create(bs,TweenInfo.new(0.15),{Color=bc,Thickness=2}):Play()T:Create(un,TweenInfo.new(0.15),{TextColor3=Color3.fromRGB(0,0,0)}):Play()sn("üéØ","Selected: "..tp.Name,1.5)end)bt[tp]=b;return b end

local function rpb(tp,lt)local bt=lt=="H"and plh or plb;if bt[tp]then bt[tp]:Destroy()bt[tp]=nil;if lt=="H"and sph==tp then sph=nil elseif lt=="B"and spb==tp then spb=nil end end end

local function upl(lt)local cp={}local sf=lt=="H"and hs or bs;local bt=lt=="H"and plh or plb;local bc=lt=="H"and Color3.fromRGB(50,255,120)or Color3.fromRGB(255,100,255)for _,x in pairs(P:GetPlayers())do if x~=p then cp[x]=true;if not bt[x]then cpb(x,sf,lt,bc)end end end;for x,_ in pairs(bt)do if not cp[x]then rpb(x,lt)end end end

spawn(function()while wait(0.5)do upl("H")upl("B")end end)

local function tf(s,st)st.fly=not st.fly;if st.fly then local bv=Instance.new("BodyVelocity")bv.Name="FV"bv.MaxForce=Vector3.new(9e9,9e9,9e9)bv.Velocity=Vector3.new(0,0,0)bv.Parent=r;local bg=Instance.new("BodyGyro")bg.Name="FG"bg.MaxForce=Vector3.new(9e9,9e9,9e9)bg.P=9e4;bg.CFrame=r.CFrame;bg.Parent=r;cn.fly=R.Heartbeat:Connect(function()if not st.fly then return end;local md=Vector3.new(0,0,0)if U:IsKeyDown(Enum.KeyCode.W)then md=md+c.CFrame.LookVector end;if U:IsKeyDown(Enum.KeyCode.S)then md=md-c.CFrame.LookVector end;if U:IsKeyDown(Enum.KeyCode.A)then md=md-c.CFrame.RightVector end;if U:IsKeyDown(Enum.KeyCode.D)then md=md+c.CFrame.RightVector end;if U:IsKeyDown(Enum.KeyCode.Space)then md=md+Vector3.new(0,1,0)end;if U:IsKeyDown(Enum.KeyCode.LeftShift)then md=md-Vector3.new(0,1,0)end;if md.Magnitude>0 then md=md.Unit end;bv.Velocity=md*fs;bg.CFrame=c.CFrame end)s.Text="ON"s.BackgroundColor3=Color3.fromRGB(50,255,120)st.TextColor3=Color3.fromRGB(0,0,0)sn("‚úÖ","Fly ON",2)else if cn.fly then cn.fly:Disconnect()end;if r:FindFirstChild("FV")then r.FV:Destroy()end;if r:FindFirstChild("FG")then r.FG:Destroy()end;s.Text="OFF"s.BackgroundColor3=Color3.fromRGB(60,60,60)st.TextColor3=Color3.fromRGB(200,200,200)sn("‚ùå","Fly OFF",1.5)end end

local function tn(s,st)st.noclip=not st.noclip;if st.noclip then oc={}np={}for _,x in pairs(ch:GetDescendants())do if x:IsA("BasePart")then table.insert(np,x)oc[x]=x.CanCollide end end;cn.noclip=R.RenderStepped:Connect(function()if not st.noclip then return end;for _,x in pairs(np)do if x and x.Parent then pcall(function()x.CanCollide=false end)end end end)s.Text="ON"s.BackgroundColor3=Color3.fromRGB(50,255,120)st.TextColor3=Color3.fromRGB(0,0,0)sn("‚úÖ","Noclip ON",2)else if cn.noclip then cn.noclip:Disconnect()end;task.spawn(function()for x,o in pairs(oc)do if x and x.Parent then pcall(function()x.CanCollide=o end)task.wait(0.01)end end end)oc={}np={}s.Text="OFF"s.BackgroundColor3=Color3.fromRGB(60,60,60)st.TextColor3=Color3.fromRGB(200,200,200)sn("‚ùå","Noclip OFF",1.5)end end

local function ts(s,st)st.speed=not st.speed;if st.speed then h.WalkSpeed=sv;cn.speed=h:GetPropertyChangedSignal("WalkSpeed"):Connect(function()if st.speed then h.WalkSpeed=sv end end)s.Text="ON"s.BackgroundColor3=Color3.fromRGB(50,255,120)st.TextColor3=Color3.fromRGB(0,0,0)else if cn.speed then cn.speed:Disconnect()end;h.WalkSpeed=16;s.Text="OFF"s.BackgroundColor3=Color3.fromRGB(60,60,60)st.TextColor3=Color3.fromRGB(200,200,200)end end

local function tj(s,st)st.jump=not st.jump;if st.jump then h.JumpPower=jv;cn.jump=h:GetPropertyChangedSignal("JumpPower"):Connect(function()if st.jump then h.JumpPower=jv end end)s.Text="ON"s.BackgroundColor3=Color3.fromRGB(50,255,120)st.TextColor3=Color3.fromRGB(0,0,0)else if cn.jump then cn.jump:Disconnect()end;h.JumpPower=50;s.Text="OFF"s.BackgroundColor3=Color3.fromRGB(60,60,60)st.TextColor3=Color3.fromRGB(200,200,200)end end

local function tij(s,st)st.ijump=not st.ijump;if st.ijump then cn.ijump=U.JumpRequest:Connect(function()if st.ijump then h:ChangeState(Enum.HumanoidStateType.Jumping)end end)s.Text="ON"s.BackgroundColor3=Color3.fromRGB(50,255,120)st.TextColor3=Color3.fromRGB(0,0,0)else if cn.ijump then cn.ijump:Disconnect()end;s.Text="OFF"s.BackgroundColor3=Color3.fromRGB(60,60,60)st.TextColor3=Color3.fromRGB(200,200,200)end end

local function ttp(s,st)if not sph then sn("‚ö†Ô∏è","No Player",2)return end;if sph.Character and sph.Character:FindFirstChild("HumanoidRootPart")then local tr=sph.Character.HumanoidRootPart;r.CFrame=tr.CFrame*CFrame.new(0,0,3)sn("‚úÖ","TP: "..sph.Name,2)else sn("‚ö†Ô∏è","Failed",2)end end

local function thp(s,st)st.hump=not st.hump;if st.hump then if not sph then st.hump=false;sn("‚ö†Ô∏è","No Player",2)return end;cn.hump=R.Heartbeat:Connect(function()if not st.hump or not sph then if cn.hump then cn.hump:Disconnect()end;return end;if sph.Character and sph.Character:FindFirstChild("HumanoidRootPart")then local tr=sph.Character.HumanoidRootPart;local t=tick()local yo=math.sin(t*8)*0.3;local zo=math.sin(t*8)*0.5;r.CFrame=tr.CFrame*CFrame.new(0,yo,1.5+zo)local lv=(tr.Position-r.Position).Unit;r.CFrame=CFrame.new(r.Position,r.Position+lv)if h then h:MoveTo(tr.Position)end else st.hump=false;s.Text="OFF"s.BackgroundColor3=Color3.fromRGB(60,60,60)st.TextColor3=Color3.fromRGB(200,200,200)end end)s.Text="ON"s.BackgroundColor3=Color3.fromRGB(50,255,120)st.TextColor3=Color3.fromRGB(0,0,0)sn("üòà","Humping: "..sph.Name,2)else if cn.hump then cn.hump:Disconnect()end;s.Text="OFF"s.BackgroundColor3=Color3.fromRGB(60,60,60)st.TextColor3=Color3.fromRGB(200,200,200)end end

local function tbs(s,st)if not spb then sn("‚ö†Ô∏è","No Player",2)return end;if spb.Character and spb.Character:FindFirstChild("HumanoidRootPart")then local tr=spb.Character.HumanoidRootPart;tr.CFrame=r.CFrame*CFrame.new(0,0,-3)sn("‚úÖ","Brought: "..spb.Name,2)else sn("‚ö†Ô∏è","Failed",2)end end

