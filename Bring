-- GIANT MODE
createButton(visualPage, "GIANT Mode (Make Yourself HUGE)", "ğŸ—¿", 3, function(s, st)
    states.giant = not states.giant
    
    if states.giant then
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                part.Size = part.Size * 3
            end
        end
        humanoid.HipHeight = humanoid.HipHeight * 3
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
        notify("ğŸ—¿", "GIANT MODE!", 2)
    else
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") and originalSizes[part] then
                part.Size = originalSizes[part].Size
            end
        end
        humanoid.HipHeight = 0
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

