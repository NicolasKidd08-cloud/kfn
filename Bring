-- Notification System
local function sendNotification(title, text, duration)
    duration = duration or 3
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration,
        Icon = "rbxassetid://2541869220"
    })
end

-- Config System
local function saveConfig(configName)
    local config = {
        states = states,
        flySpeed = flySpeed,
        speedValue = speedValue,
        jumpValue = jumpValue,
        fovValue = fovValue,
        keybinds = keybinds
    }
    
    local success, err = pcall(function()
        writefile(configName .. ".json", HttpService:JSONEncode(config))
    end)
    
    if success then
        sendNotification("ðŸ’¾ Config Saved", "Saved as: " .. configName, 2)
    else
        sendNotification("âŒ Save Failed", "Error saving config", 2)
    end
end

local function loadConfig(configName)
    local success, result = pcall(function()
        return HttpService:JSONDecode(readfile(configName .. ".json"))
    end)
    
    if success and result then
        states = result.states or states
        flySpeed = result.flySpeed or flySpeed
        speedValue = result.speedValue or speedValue
        jumpValue = result.jumpValue or jumpValue
        fovValue = result.fovValue or fovValue
        keybinds = result.keybinds or keybinds
        sendNotification("ðŸ“‚ Config Loaded", "Loaded: " .. configName, 2)
    else
        sendNotification("âŒ Load Failed", "Config not found", 2)
    end
end

