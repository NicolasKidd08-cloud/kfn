    states.jump = not states.jump
    
    if states.jump then
        humanoid.JumpPower = 200
        connections.jump = humanoid:GetPropertyChangedSignal("JumpPower"):Connect(function()
            if states.jump then humanoid.JumpPower = 200 end
        end)
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if connections.jump then connections.jump:Disconnect() end
        humanoid.JumpPower = 50
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- INFINITE JUMP
createButton(movementPage, "Infinite Jump", "â™¾ï¸", 5, function(s, st)
    states.infiniteJump = not states.infiniteJump
    
    if states.infiniteJump then
        connections.infiniteJump = UserInputService.JumpRequest:Connect(function()
            if states.infiniteJump then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if connections.infiniteJump then connections.infiniteJump:Disconnect() end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- JESUS WALK (Walk on Water)
createButton(movementPage, "Jesus Walk (Walk on Water)", "ğŸŒŠ", 6, function(s, st)
    states.jesus = not states.jesus
    
    if states.jesus then
        local platform = Instance.new("Part")
        platform.Name = "WaterPlatform"
        platform.Size = Vector3.new(10, 0.5, 10)
        platform.Anchored = true
        platform.Transparency = 1
        platform.CanCollide = true
        platform.Parent = Workspace
        
        connections.jesus = RunService.Heartbeat:Connect(function()
            if states.jesus and rootPart then
                platform.CFrame = CFrame.new(rootPart.Position.X, rootPart.Position.Y - 3.5, rootPart.Position.Z)
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if connections.jesus then connections.jesus:Disconnect() end
        if Workspace:FindFirstChild("WaterPlatform") then
            Workspace.WaterPlatform:Destroy()
        end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

