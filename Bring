-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- UI CONTROLS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local isMinimized = false
toggleButton.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    
    if isMinimized then
        TweenService:Create(contentFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            Size = UDim2.new(1, 0, 0, 0)
        }):Play()
        TweenService:Create(toggleButton, TweenInfo.new(0.3), {
            Rotation = 180
        }):Play()
        toggleButton.Text = "+"
    else
        TweenService:Create(contentFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            Size = UDim2.new(1, 0, 1, -75)
        }):Play()
        TweenService:Create(toggleButton, TweenInfo.new(0.3), {
            Rotation = 0
        }):Play()
        toggleButton.Text = "âˆ’"
    end
end)

-- Draggable for main UI
local dragging = false
local dragInput, dragStart, startPos

local function update(input)
    local delta = input.Position - dragStart
    mainContainer.Position = UDim2.new(
        startPos.X.Scale,
        startPos.X.Offset + delta.X,
        startPos.Y.Scale,
        startPos.Y.Offset + delta.Y
    )
end

headerFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or 
       input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = mainContainer.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

headerFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or
       input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        update(input)
    end
end)

-- Draggable for player lists (same pattern)
local draggingHump = false
local dragInputHump, dragStartHump, startPosHump

local function updateHump(input)
    local delta = input.Position - dragStartHump
    playerListHumpContainer.Position = UDim2.new(
        startPosHump.X.Scale,
        startPosHump.X.Offset + delta.X,
        startPosHump.Y.Scale,
        startPosHump.Y.Offset + delta.Y
    )
end

humpHeader.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or 
       input.UserInputType == Enum.UserInputType.Touch then
        draggingHump = true
        dragStartHump = input.Position
        startPosHump = playerListHumpContainer.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                draggingHump = false
            end
        end)
    end
end)

humpHeader.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or
       input.UserInputType == Enum.UserInputType.Touch then
        dragInputHump = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInputHump and draggingHump then
        updateHump(input)
    end
end)

local draggingBring = false
local dragInputBring, dragStartBring, startPosBring

local function updateBring(input)
    local delta = input.Position - dragStartBring
    playerListBringContainer.Position = UDim2.new(
        startPosBring.X.Scale,
        startPosBring.X.Offset + delta.X,
        startPosBring.Y.Scale,
        startPosBring.Y.Offset + delta.Y
    )
end

bringHeader.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or 
       input.UserInputType == Enum.UserInputType.Touch then
        draggingBring = true
        dragStartBring = input.Position
        startPosBring = playerListBringContainer.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                draggingBring = false
            end
        end)
    end
end)

bringHeader.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or
       input.UserInputType == Enum.UserInputType.Touch then
        dragInputBring = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInputBring and draggingBring then
        updateBring(input)
    end
end)

-- Player connections
Players.PlayerAdded:Connect(function(p)
    updatePlayerList("Hump")
    updatePlayerList("Bring")
    
    if states.nameESP or states.boxESP or states.distanceESP or states.tracers or states.healthESP or states.chamsESP then
        p.CharacterAdded:Connect(function()
            wait(0.5)
            createESPForPlayer(p)
        end)
    end
end)

Players.PlayerRemoving:Connect(function(p)
    removePlayerButton(p, "Hump")
    removePlayerButton(p, "Bring")
    removeESPForPlayer(p)
end)

connections.espUpdate = RunService.RenderStepped:Connect(updateESP)

-- Character respawn handler
player.CharacterAdded:Connect(function(newChar)
    character = newChar
    humanoid = newChar:WaitForChild("Humanoid")
    rootPart = newChar:WaitForChild("HumanoidRootPart")
    
    for key, _ in pairs(states) do
        states[key] = false
    end
    
    for _, conn in pairs(connections) do
        if conn then conn:Disconnect() end
    end
    
    connections = {}
    originalCanCollide = {}
    noclipParts = {}
    
    sendNotification("ðŸ”„ Character Respawned", "All features reset", 2)
end)

-- Initial setup
updatePlayerList("Hump")
updatePlayerList("Bring")

-- Entrance animations
mainContainer.Position = UDim2.new(0.5, -300, 1.5, 0)
mainContainer.Size = UDim2.new(0, 0, 0, 0)
playerListHumpContainer.Position = UDim2.new(0.5, 310, 1.5, 0)
playerListHumpContainer.Size = UDim2.new(0, 0, 0, 0)
playerListBringContainer.Position = UDim2.new(0.5, -590, 1.5, 0)
playerListBringContainer.Size = UDim2.new(0, 0, 0, 0)

wait(0.1)

TweenService:Create(mainContainer, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Size = UDim2.new(0, 600, 0, 550)
}):Play()
TweenService:Create(mainContainer, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Position = UDim2.new(0.5, -300, 0.5, -275)
}):Play()

wait(0.2)

TweenService:Create(playerListHumpContainer, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Size = UDim2.new(0, 280, 0, 550)
}):Play()
TweenService:Create(playerListHumpContainer, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Position = UDim2.new(0.5, 310, 0.5, -275)
}):Play()

wait(0.2)

TweenService:Create(playerListBringContainer, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Size = UDim2.new(0, 280, 0, 550)
}):Play()
TweenService:Create(playerListBringContainer, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Position = UDim2.new(0.5, -590, 0.5, -275)
}):Play()

-- Send welcome notification
wait(1)
sendNotification("ðŸ¦› MATTHEW HIPPO UI", "Ultimate Edition Loaded!", 3)
wait(1)
sendNotification("ðŸ’š Thank You!", "Made by Skinny Nicolas", 2)

