plLayout.SortOrder = Enum.SortOrder.Name
plLayout.Padding = UDim.new(0, 3)
plLayout.Parent = plScroll

local function createPlayerButton(targetPlayer)
    if playerButtons[targetPlayer] then return end
    
    local button = Instance.new("TextButton")
    button.Name = targetPlayer.Name
    button.Size = UDim2.new(1, -5, 0, 45)
    button.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    button.BorderSizePixel = 0
    button.Text = "  " .. targetPlayer.Name
    button.TextSize = 13
    button.TextColor3 = Color3.fromRGB(220, 220, 220)
    button.Font = Enum.Font.GothamBold
    button.TextXAlignment = Enum.TextXAlignment.Left
    button.Parent = plScroll
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 8)
    btnCorner.Parent = button
    
    button.MouseButton1Click:Connect(function()
        if selectedPlayer and playerButtons[selectedPlayer] then
            playerButtons[selectedPlayer].BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        end
        
        selectedPlayer = targetPlayer
        button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        notify("ğŸ¯ Selected", targetPlayer.Name, 1.5)
    end)
    
    playerButtons[targetPlayer] = button
end

local function updatePlayerList()
    for player, button in pairs(playerButtons) do
        if not Players:FindFirstChild(player.Name) then
            button:Destroy()
            playerButtons[player] = nil
            if selectedPlayer == player then
                selectedPlayer = nil
            end
        end
    end
    
    for _, targetPlayer in pairs(Players:GetPlayers()) do
        if targetPlayer ~= player then
            createPlayerButton(targetPlayer)
        end
    end
end

spawn(function()
    while wait(1) do
        pcall(updatePlayerList)
    end
end)

updatePlayerList()

