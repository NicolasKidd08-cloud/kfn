createToggleButton(mainPage, "God Mode", "ðŸ›¡ï¸", 15, toggleGodMode)
createToggleButton(mainPage, "No Rotate (Lock Rotation)", "ðŸ”’", 16, toggleNoRotate)

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- VISUALS PAGE
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function toggleFullBright(status, statusText)
    states.fullBright = not states.fullBright
    
    if states.fullBright then
        Lighting.Ambient = Color3.new(1, 1, 1)
        Lighting.Brightness = 2
        Lighting.ColorShift_Bottom = Color3.new(1, 1, 1)
        Lighting.ColorShift_Top = Color3.new(1, 1, 1)
        Lighting.OutdoorAmbient = Color3.new(1, 1, 1)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        Lighting.Ambient = originalLighting.Ambient
        Lighting.Brightness = originalLighting.Brightness
        Lighting.ColorShift_Bottom = originalLighting.ColorShift_Bottom
        Lighting.ColorShift_Top = originalLighting.ColorShift_Top
        Lighting.OutdoorAmbient = originalLighting.OutdoorAmbient
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

local function toggleRemoveFog(status, statusText)
    states.removeFog = not states.removeFog
    
    if states.removeFog then
        Lighting.FogEnd = 100000
        Lighting.FogStart = 0
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        Lighting.FogEnd = originalLighting.FogEnd
        Lighting.FogStart = originalLighting.FogStart
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

local function toggleFOV(status, statusText)
    states.fov = not states.fov
    
    if states.fov then
        camera.FieldOfView = fovValue
        connections.fov = camera:GetPropertyChangedSignal("FieldOfView"):Connect(function()
            if states.fov then
                camera.FieldOfView = fovValue
            end
        end)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.fov then connections.fov:Disconnect() end
        camera.FieldOfView = 70
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

createToggleButton(visualsPage, "FullBright (See Everything)", "ðŸ’¡", 1, toggleFullBright)
createToggleButton(visualsPage, "Remove Fog", "ðŸŒ«ï¸", 2, toggleRemoveFog)
createToggleButton(visualsPage, "FOV Changer (120 FOV)", "ðŸŽ¥", 3, toggleFOV)

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- ESP PAGE (Enhanced)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function createESPForPlayer(targetPlayer)
    if targetPlayer == player then return end
    if espObjects[targetPlayer] then return end
    
    local espFolder = Instance.new("Folder")
    espFolder.Name = "ESP_" .. targetPlayer.Name
    espFolder.Parent = camera
    
    espObjects[targetPlayer] = {
        folder = espFolder,
        nameLabel = nil,
        boxAdornment = nil,
        tracer = nil,
        distanceLabel = nil,
        healthLabel = nil,
        chams = {}
    }
    
    if states.nameESP then
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "NameESP"
        billboard.AlwaysOnTop = true
        billboard.Size = UDim2.new(0, 200, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.Parent = espFolder
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(1, 0, 1, 0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = targetPlayer.Name
        nameLabel.TextColor3 = Color3.fromRGB(50, 255, 120)
        nameLabel.TextStrokeTransparency = 0.5
        nameLabel.TextSize = 14
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.Parent = billboard
        
        espObjects[targetPlayer].nameLabel = billboard
    end
    
    if states.boxESP then
        local box = Instance.new("BoxHandleAdornment")
        box.Name = "BoxESP"
        box.AlwaysOnTop = true
        box.Color3 = Color3.fromRGB(50, 255, 120)
        box.ZIndex = 1
        box.Transparency = 0.7
        box.Size = Vector3.new(4, 5, 1)
        box.Parent = espFolder
        
        espObjects[targetPlayer].boxAdornment = box
    end
    
    if states.tracers then
        local line = Drawing.new("Line")
        line.Visible = true
        line.Color = Color3.new(50/255, 1, 120/255)
        line.Thickness = 2
        line.Transparency = 1
        
        espObjects[targetPlayer].tracer = line
    end
    
    if states.distanceESP then
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "DistanceESP"
        billboard.AlwaysOnTop = true
        billboard.Size = UDim2.new(0, 200, 0, 30)
        billboard.StudsOffset = Vector3.new(0, -2, 0)
        billboard.Parent = espFolder
        
        local distLabel = Instance.new("TextLabel")
        distLabel.Size = UDim2.new(1, 0, 1, 0)
        distLabel.BackgroundTransparency = 1
        distLabel.Text = "0m"
        distLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
        distLabel.TextStrokeTransparency = 0.5
        distLabel.TextSize = 12
        distLabel.Font = Enum.Font.Gotham
        distLabel.Parent = billboard
        
        espObjects[targetPlayer].distanceLabel = billboard
    end
    
    if states.healthESP and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "HealthESP"
        billboard.AlwaysOnTop = true
        billboard.Size = UDim2.new(0, 200, 0, 30)
        billboard.StudsOffset = Vector3.new(0, -3.5, 0)
        billboard.Parent = espFolder
        
        local healthLabel = Instance.new("TextLabel")
        healthLabel.Size = UDim2.new(1, 0, 1, 0)
        healthLabel.BackgroundTransparency = 1
        healthLabel.Text = "HP: 100"
        healthLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
        healthLabel.TextStrokeTransparency = 0.5
        healthLabel.TextSize = 12
        healthLabel.Font = Enum.Font.Gotham
        healthLabel.Parent = billboard
        
        espObjects[targetPlayer].healthLabel = billboard
    end
    
    if states.chamsESP and targetPlayer.Character then
        for _, part in pairs(targetPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                local highlight = Instance.new("BoxHandleAdornment")
                highlight.AlwaysOnTop = true
                highlight.ZIndex = 5
                highlight.Size = part.Size
                highlight.Color3 = Color3.fromRGB(50, 255, 120)
                highlight.Transparency = 0.5
                highlight.Adornee = part
                highlight.Parent = espFolder
                
                table.insert(espObjects[targetPlayer].chams, highlight)
            end
        end
    end
end

local function removeESPForPlayer(targetPlayer)
    if espObjects[targetPlayer] then
        if espObjects[targetPlayer].folder then
            espObjects[targetPlayer].folder:Destroy()
        end
        if espObjects[targetPlayer].tracer then
            espObjects[targetPlayer].tracer:Remove()
        end
        espObjects[targetPlayer] = nil
    end
end

local function updateESP()
    for targetPlayer, esp in pairs(espObjects) do
        if targetPlayer and targetPlayer.Character then
            local targetRoot = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
            local targetHumanoid = targetPlayer.Character:FindFirstChild("Humanoid")
            
            if targetRoot and targetHumanoid and targetHumanoid.Health > 0 then
                if esp.nameLabel then
                    esp.nameLabel.Adornee = targetRoot
                end
                
                if esp.boxAdornment then
                    esp.boxAdornment.Adornee = targetRoot
                end
                
                if esp.distanceLabel then
                    esp.distanceLabel.Adornee = targetRoot
                    local distance = (rootPart.Position - targetRoot.Position).Magnitude
                    esp.distanceLabel.TextLabel.Text = math.floor(distance) .. "m"
                end
                
                if esp.healthLabel then
                    esp.healthLabel.Adornee = targetRoot
                    local health = math.floor(targetHumanoid.Health)
                    esp.healthLabel.TextLabel.Text = "HP: " .. health
                    
                    local healthPercent = health / targetHumanoid.MaxHealth
                    if healthPercent > 0.5 then
                        esp.healthLabel.TextLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
                    elseif healthPercent > 0.25 then
                        esp.healthLabel.TextLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
                    else
                        esp.healthLabel.TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
                    end
                end
                
                if esp.tracer then
                    local screenPos, onScreen = camera:WorldToViewportPoint(targetRoot.Position)
                    if onScreen then
                        esp.tracer.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y)
                        esp.tracer.To = Vector2.new(screenPos.X, screenPos.Y)
                        esp.tracer.Visible = true
                    else
                        esp.tracer.Visible = false
                    end
                end
            else
                removeESPForPlayer(targetPlayer)
            end
        else
            removeESPForPlayer(targetPlayer)
        end
    end
end

local function toggleNameESP(status, statusText)
    states.nameESP = not states.nameESP
    
    if states.nameESP then
        for _, p in pairs(Players:GetPlayers()) do
            createESPForPlayer(p)
        end
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        for p, esp in pairs(espObjects) do
            if esp.nameLabel then
                esp.nameLabel:Destroy()
                esp.nameLabel = nil
            end
        end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

local function toggleBoxESP(status, statusText)
    states.boxESP = not states.boxESP
    
    if states.boxESP then
        for _, p in pairs(Players:GetPlayers()) do
            createESPForPlayer(p)
        end
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        for p, esp in pairs(espObjects) do
            if esp.boxAdornment then
                esp.boxAdornment:Destroy()
                esp.boxAdornment = nil
            end
        end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

local function toggleDistanceESP(status, statusText)
    states.distanceESP = not states.distanceESP
    
    if states.distanceESP then
        for _, p in pairs(Players:GetPlayers()) do
            createESPForPlayer(p)
        end
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        for p, esp in pairs(espObjects) do
            if esp.distanceLabel then
                esp.distanceLabel:Destroy()
                esp.distanceLabel = nil
            end
        end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

local function toggleTracers(status, statusText)
    states.tracers = not states.tracers
    
    if states.tracers then
        for _, p in pairs(Players:GetPlayers()) do
            createESPForPlayer(p)
        end
        connections.tracerUpdate = RunService.RenderStepped:Connect(updateESP)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        for p, esp in pairs(espObjects) do
            if esp.tracer then
                esp.tracer:Remove()
                esp.tracer = nil
            end
        end
        if connections.tracerUpdate then
            connections.tracerUpdate:Disconnect()
        end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

local function toggleHealthESP(status, statusText)
    states.healthESP = not states.healthESP
    
    if states.healthESP then
        for _, p in pairs(Players:GetPlayers()) do
            createESPForPlayer(p)
        end
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        for p, esp in pairs(espObjects) do
            if esp.healthLabel then
                esp.healthLabel:Destroy()
                esp.healthLabel = nil
            end
        end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

local function toggleChamsESP(status, statusText)
    states.chamsESP = not states.chamsESP
    
    if states.chamsESP then
        for _, p in pairs(Players:GetPlayers()) do
            createESPForPlayer(p)
        end
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        for p, esp in pairs(espObjects) do
            if esp.chams then
                for _, cham in pairs(esp.chams) do
                    cham:Destroy()
                end
                esp.chams = {}
            end
        end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

