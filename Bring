createToggleButton(mainPage, "Fly (Speed: 200)", "ðŸ•Šï¸", 1, toggleFly)
createToggleButton(mainPage, "Noclip (Anti-Cheat Resistant)", "ðŸ‘»", 2, toggleNoclip)
