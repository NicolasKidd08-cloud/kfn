local ct,cp=mt,mp;mp.Visible=true

local function sw(t,p)for _,x in pairs(tabr:GetChildren())do if x:IsA("TextButton")then x.BackgroundColor3=Color3.fromRGB(30,30,30)x.TextColor3=Color3.fromRGB(180,180,180)end end;for _,x in pairs(pc:GetChildren())do if x:IsA("ScrollingFrame")then x.Visible=false end end;t.BackgroundColor3=Color3.fromRGB(50,255,120)t.TextColor3=Color3.fromRGB(0,0,0)p.Visible=true;ct=t;cp=p end

mt.MouseButton1Click:Connect(function()sw(mt,mp)end)vt.MouseButton1Click:Connect(function()sw(vt,vp)end)et.MouseButton1Click:Connect(function()sw(et,ep)end)sct.MouseButton1Click:Connect(function()sw(sct,scp)end)mit.MouseButton1Click:Connect(function()sw(mit,mip)end)

