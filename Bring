    "ðŸ˜ˆ HUMP LIST",
    Color3.fromRGB(50, 255, 120),
    "Hump"
)

local bringHeader, bringScroll = createPlayerListUI(
    playerListBringContainer,
    "ðŸŽ¯ BRING LIST",
    Color3.fromRGB(255, 100, 255),
    "Bring"
)

local function createPlayerButton(targetPlayer, scrollFrame, listType, buttonColor)
    local buttonTable = listType == "Hump" and playerListButtonsHump or playerListButtonsBring
    
    if buttonTable[targetPlayer] then
        return buttonTable[targetPlayer]
    end
    
    local button = Instance.new("TextButton")
    button.Name = targetPlayer.Name
    button.Size = UDim2.new(1, -5, 0, 50)
    button.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    button.BorderSizePixel = 0
    button.Text = ""
    button.AutoButtonColor = false
    button.Parent = scrollFrame
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 8)
    buttonCorner.Parent = button
    
    local buttonStroke = Instance.new("UIStroke")
    buttonStroke.Color = Color3.fromRGB(40, 40, 40)
    buttonStroke.Thickness = 1
    buttonStroke.Parent = button
    
    local profileImage = Instance.new("ImageLabel")
    profileImage.Size = UDim2.new(0, 40, 0, 40)
    profileImage.Position = UDim2.new(0, 5, 0, 5)
    profileImage.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    profileImage.BorderSizePixel = 0
    profileImage.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. targetPlayer.UserId .. "&width=150&height=150&format=png"
    profileImage.Parent = button
    
    local profileCorner = Instance.new("UICorner")
    profileCorner.CornerRadius = UDim.new(0, 8)
    profileCorner.Parent = profileImage
    
    local username = Instance.new("TextLabel")
    username.Size = UDim2.new(1, -55, 1, 0)
    username.Position = UDim2.new(0, 50, 0, 0)
    username.BackgroundTransparency = 1
    username.Text = targetPlayer.Name
    username.TextSize = 14
    username.TextColor3 = Color3.fromRGB(220, 220, 220)
    username.Font = Enum.Font.GothamBold
    username.TextXAlignment = Enum.TextXAlignment.Left
    username.TextTruncate = Enum.TextTruncate.AtEnd
    username.Parent = button
    
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(35, 35, 35)}):Play()
        TweenService:Create(buttonStroke, TweenInfo.new(0.15), {Color = buttonColor}):Play()
    end)
    
    button.MouseLeave:Connect(function()
        local selectedPlayer = listType == "Hump" and selectedPlayerHump or selectedPlayerBring
        if selectedPlayer ~= targetPlayer then
            TweenService:Create(button, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(25, 25, 25)}):Play()
            TweenService:Create(buttonStroke, TweenInfo.new(0.15), {Color = Color3.fromRGB(40, 40, 40)}):Play()
        end
    end)
    
    button.MouseButton1Click:Connect(function()
        local selectedPlayer = listType == "Hump" and selectedPlayerHump or selectedPlayerBring
        if selectedPlayer and buttonTable[selectedPlayer] then
            local prevButton = buttonTable[selectedPlayer]
            TweenService:Create(prevButton, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(25, 25, 25)}):Play()
            local prevStroke = prevButton:FindFirstChildOfClass("UIStroke")
            if prevStroke then
                TweenService:Create(prevStroke, TweenInfo.new(0.15), {Color = Color3.fromRGB(40, 40, 40)}):Play()
            end
            local prevUsername = prevButton:FindFirstChild("TextLabel")
            if prevUsername then
                TweenService:Create(prevUsername, TweenInfo.new(0.15), {TextColor3 = Color3.fromRGB(220, 220, 220)}):Play()
            end
        end
        
        if listType == "Hump" then
            selectedPlayerHump = targetPlayer
        else
            selectedPlayerBring = targetPlayer
        end
        
        TweenService:Create(button, TweenInfo.new(0.15), {BackgroundColor3 = buttonColor}):Play()
        TweenService:Create(buttonStroke, TweenInfo.new(0.15), {Color = buttonColor, Thickness = 2}):Play()
        TweenService:Create(username, TweenInfo.new(0.15), {TextColor3 = Color3.fromRGB(0, 0, 0)}):Play()
        
        sendNotification("ðŸŽ¯ Selected", targetPlayer.Name .. " (" .. listType .. " List)", 1.5)
    end)
    
    buttonTable[targetPlayer] = button
    return button
end

local function removePlayerButton(targetPlayer, listType)
    local buttonTable = listType == "Hump" and playerListButtonsHump or playerListButtonsBring
    
    if buttonTable[targetPlayer] then
        buttonTable[targetPlayer]:Destroy()
        buttonTable[targetPlayer] = nil
        
        if listType == "Hump" and selectedPlayerHump == targetPlayer then
            selectedPlayerHump = nil
        elseif listType == "Bring" and selectedPlayerBring == targetPlayer then
            selectedPlayerBring = nil
        end
    end
end

local function updatePlayerList(listType)
    local currentPlayers = {}
    local scrollFrame = listType == "Hump" and humpScroll or bringScroll
    local buttonTable = listType == "Hump" and playerListButtonsHump or playerListButtonsBring
    local buttonColor = listType == "Hump" and Color3.fromRGB(50, 255, 120) or Color3.fromRGB(255, 100, 255)
    
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= player then
            currentPlayers[p] = true
            if not buttonTable[p] then
                createPlayerButton(p, scrollFrame, listType, buttonColor)
            end
        end
    end
    
    for p, button in pairs(buttonTable) do
        if not currentPlayers[p] then
            removePlayerButton(p, listType)
        end
    end
end

spawn(function()
    while wait(0.5) do
        updatePlayerList("Hump")
        updatePlayerList("Bring")
    end
end)

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- MAIN FEATURES (ANTI-CHEAT RESISTANT)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

-- Advanced Fly with Anti-Cheat Bypass
local function toggleFly(status, statusText)
    states.fly = not states.fly
    
    if states.fly then
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Name = "FlyVelocity"
        bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.Parent = rootPart
        
        local bodyGyro = Instance.new("BodyGyro")
        bodyGyro.Name = "FlyGyro"
        bodyGyro.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        bodyGyro.P = 9e4
        bodyGyro.CFrame = rootPart.CFrame
        bodyGyro.Parent = rootPart
        
        connections.fly = RunService.Heartbeat:Connect(function()
            if not states.fly then return end
            
            local moveDirection = Vector3.new(0, 0, 0)
            
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                moveDirection = moveDirection + camera.CFrame.LookVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                moveDirection = moveDirection - camera.CFrame.LookVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                moveDirection = moveDirection - camera.CFrame.RightVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                moveDirection = moveDirection + camera.CFrame.RightVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                moveDirection = moveDirection + Vector3.new(0, 1, 0)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                moveDirection = moveDirection - Vector3.new(0, 1, 0)
            end
            
            if moveDirection.Magnitude > 0 then
                moveDirection = moveDirection.Unit
            end
            
            bodyVelocity.Velocity = moveDirection * flySpeed
            bodyGyro.CFrame = camera.CFrame
        end)
        
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
        sendNotification("âœ… Fly Enabled", "Use WASD + Space/Shift", 2)
    else
        if connections.fly then connections.fly:Disconnect() end
        if rootPart:FindFirstChild("FlyVelocity") then rootPart.FlyVelocity:Destroy() end
        if rootPart:FindFirstChild("FlyGyro") then rootPart.FlyGyro:Destroy() end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
        sendNotification("âŒ Fly Disabled", "", 1.5)
    end
end

-- Advanced Noclip with Anti-Cheat Bypass
local function toggleNoclip(status, statusText)
    states.noclip = not states.noclip
    
    if states.noclip then
        originalCanCollide = {}
        noclipParts = {}
        
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                table.insert(noclipParts, part)
                originalCanCollide[part] = part.CanCollide
            end
        end
        
        connections.noclip = RunService.RenderStepped:Connect(function()
            if not states.noclip then return end
            for _, part in pairs(noclipParts) do
                if part and part.Parent then
                    pcall(function() part.CanCollide = false end)
                end
            end
        end)
        
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
        sendNotification("âœ… Noclip Enabled", "Walk through walls", 2)
    else
        if connections.noclip then connections.noclip:Disconnect() end
        task.spawn(function()
            for part, original in pairs(originalCanCollide) do
                if part and part.Parent then
                    pcall(function() part.CanCollide = original end)
                    task.wait(0.01)
                end
            end
        end)
        originalCanCollide = {}
        noclipParts = {}
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
        sendNotification("âŒ Noclip Disabled", "", 1.5)
    end
end

-- Speed Hack
local function toggleSpeed(status, statusText)
    states.speed = not states.speed
    
    if states.speed then
        humanoid.WalkSpeed = speedValue
        connections.speed = humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
            if states.speed then
                humanoid.WalkSpeed = speedValue
            end
        end)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.speed then connections.speed:Disconnect() end
        humanoid.WalkSpeed = 16
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Jump Power
local function toggleJumpPower(status, statusText)
    states.jumpPower = not states.jumpPower
    
    if states.jumpPower then
        humanoid.JumpPower = jumpValue
        connections.jumpPower = humanoid:GetPropertyChangedSignal("JumpPower"):Connect(function()
            if states.jumpPower then
                humanoid.JumpPower = jumpValue
            end
        end)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.jumpPower then connections.jumpPower:Disconnect() end
        humanoid.JumpPower = 50
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Infinite Jump
local function toggleInfiniteJump(status, statusText)
    states.infiniteJump = not states.infiniteJump
    
    if states.infiniteJump then
        connections.infiniteJump = UserInputService.JumpRequest:Connect(function()
            if states.infiniteJump then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.infiniteJump then connections.infiniteJump:Disconnect() end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- TP to Selected Player
local function toggleTPToPlayer(status, statusText)
    if not selectedPlayerHump then
        sendNotification("âš ï¸ No Player Selected", "Select from HUMP LIST first", 2)
        return
    end
    
    if selectedPlayerHump.Character and selectedPlayerHump.Character:FindFirstChild("HumanoidRootPart") then
        local targetRoot = selectedPlayerHump.Character.HumanoidRootPart
        rootPart.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 3)
        sendNotification("âœ… Teleported", "To: " .. selectedPlayerHump.Name, 2)
    else
        sendNotification("âš ï¸ Failed", "Player has no character", 2)
    end
end

-- Hump Selected Player
local function toggleHumpPlayer(status, statusText)
    states.humpPlayer = not states.humpPlayer
    
    if states.humpPlayer then
        if not selectedPlayerHump then
            states.humpPlayer = false
            sendNotification("âš ï¸ No Player Selected", "Select from HUMP LIST first", 2)
            return
        end
        
        connections.humpPlayer = RunService.Heartbeat:Connect(function()
            if not states.humpPlayer or not selectedPlayerHump then
                if connections.humpPlayer then
                    connections.humpPlayer:Disconnect()
                end
                return
            end
            
            if selectedPlayerHump.Character and selectedPlayerHump.Character:FindFirstChild("HumanoidRootPart") then
                local targetRoot = selectedPlayerHump.Character.HumanoidRootPart
                
                local time = tick()
                local yOffset = math.sin(time * 8) * 0.3
                local zOffset = math.sin(time * 8) * 0.5
                
                rootPart.CFrame = targetRoot.CFrame * CFrame.new(0, yOffset, 1.5 + zOffset)
                
                local lookVector = (targetRoot.Position - rootPart.Position).Unit
                rootPart.CFrame = CFrame.new(rootPart.Position, rootPart.Position + lookVector)
                
                if humanoid then
                    humanoid:MoveTo(targetRoot.Position)
                end
            else
                states.humpPlayer = false
                statusText.Text = "OFF"
                status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
                statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
            end
        end)
        
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
        sendNotification("ðŸ˜ˆ Humping", selectedPlayerHump.Name, 2)
    else
        if connections.humpPlayer then connections.humpPlayer:Disconnect() end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Bring Selected Player
local function toggleBringSelected(status, statusText)
    if not selectedPlayerBring then
        sendNotification("âš ï¸ No Player Selected", "Select from BRING LIST first", 2)
        return
    end
    
    if selectedPlayerBring.Character and selectedPlayerBring.Character:FindFirstChild("HumanoidRootPart") then
        local targetRoot = selectedPlayerBring.Character.HumanoidRootPart
        targetRoot.CFrame = rootPart.CFrame * CFrame.new(0, 0, -3)
        sendNotification("âœ… Brought", selectedPlayerBring.Name, 2)
    else
        sendNotification("âš ï¸ Failed", "Player has no character", 2)
    end
end

-- Bring All Players
local function toggleBringAll(status, statusText)
    states.bringAll = not states.bringAll
    
    if states.bringAll then
        connections.bringAll = RunService.Heartbeat:Connect(function()
            if not states.bringAll then return end
            
            local angle = 0
            local radius = 5
            
            for _, p in pairs(Players:GetPlayers()) do
                if p ~= player and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                    local targetRoot = p.Character.HumanoidRootPart
                    
                    local x = math.cos(angle) * radius
                    local z = math.sin(angle) * radius
                    
                    targetRoot.CFrame = rootPart.CFrame * CFrame.new(x, 0, z)
                    
                    angle = angle + (math.pi * 2) / #Players:GetPlayers()
                end
            end
        end)
        
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
        sendNotification("ðŸŒ€ Bringing All", "Circle formation", 2)
    else
        if connections.bringAll then connections.bringAll:Disconnect() end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Click TP
local function toggleClickTP(status, statusText)
    states.clickTP = not states.clickTP
    
    if states.clickTP then
        connections.clickTP = mouse.Button1Down:Connect(function()
            if states.clickTP and UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
                local targetPos = mouse.Hit.Position
                rootPart.CFrame = CFrame.new(targetPos + Vector3.new(0, 3, 0))
            end
        end)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.clickTP then connections.clickTP:Disconnect() end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Anti AFK
local function toggleAntiAFK(status, statusText)
    states.antiAFK = not states.antiAFK
    
    if states.antiAFK then
        local virtualUser = game:GetService("VirtualUser")
        connections.antiAFK = player.Idled:Connect(function()
            if states.antiAFK then
                virtualUser:CaptureController()
                virtualUser:ClickButton2(Vector2.new())
            end
        end)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.antiAFK then connections.antiAFK:Disconnect() end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Walk Speed Bypass (Gradual)
local function toggleWalkSpeed(status, statusText)
    states.walkSpeed = not states.walkSpeed
    
    if states.walkSpeed then
        local targetSpeed = speedValue
        local currentSpeed = humanoid.WalkSpeed
        local increment = (targetSpeed - currentSpeed) / 10
        
        connections.walkSpeed = RunService.Heartbeat:Connect(function()
            if not states.walkSpeed then return end
            
            if math.abs(humanoid.WalkSpeed - targetSpeed) > 0.1 then
                humanoid.WalkSpeed = humanoid.WalkSpeed + increment
            else
                humanoid.WalkSpeed = targetSpeed
            end
        end)
        
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.walkSpeed then connections.walkSpeed:Disconnect() end
        humanoid.WalkSpeed = 16
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Swim Speed
local function toggleSwimSpeed(status, statusText)
    states.swimSpeed = not states.swimSpeed
    
    if states.swimSpeed then
        connections.swimSpeed = RunService.Heartbeat:Connect(function()
            if not states.swimSpeed then return end
            
            if humanoid:GetState() == Enum.HumanoidStateType.Swimming then
                humanoid.WalkSpeed = swimSpeedValue
            end
        end)
        
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.swimSpeed then connections.swimSpeed:Disconnect() end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Invisibility
local function toggleInvisibility(status, statusText)
    states.invisibility = not states.invisibility
    
    if states.invisibility then
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") or part:IsA("Decal") then
                part.Transparency = 1
            elseif part:IsA("Accessory") then
                part.Handle.Transparency = 1
            end
        end
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Transparency = part.Name == "Head" and 0 or (part.Name == "HumanoidRootPart" and 1 or 0)
            elseif part:IsA("Decal") then
                part.Transparency = 0
            elseif part:IsA("Accessory") then
                part.Handle.Transparency = 0
            end
        end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- God Mode
local function toggleGodMode(status, statusText)
    states.godMode = not states.godMode
    
    if states.godMode then
        connections.godMode = humanoid.HealthChanged:Connect(function(health)
            if states.godMode and health < humanoid.MaxHealth then
                humanoid.Health = humanoid.MaxHealth
            end
        end)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.godMode then connections.godMode:Disconnect() end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Remove Rotate
local function toggleNoRotate(status, statusText)
    states.noRotate = not states.noRotate
    
    if states.noRotate then
        rootPart.CFrame = CFrame.new(rootPart.Position) * CFrame.Angles(0, 0, 0)
        connections.noRotate = RunService.Heartbeat:Connect(function()
            if states.noRotate then
                rootPart.CFrame = CFrame.new(rootPart.Position)
            end
        end)
        statusText.Text = "ON"
        status.BackgroundColor3 = Color3.fromRGB(50, 255, 120)
        statusText.TextColor3 = Color3.fromRGB(0, 0, 0)
    else
        if connections.noRotate then connections.noRotate:Disconnect() end
        statusText.Text = "OFF"
        status.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        statusText.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end

