-- Animate gradient
spawn(function()
    while wait(3) do
        TweenService:Create(headerGradient, TweenInfo.new(3, Enum.EasingStyle.Linear), {Rotation = 360}):Play()
        wait(3)
        headerGradient.Rotation = 0
    end
end)

