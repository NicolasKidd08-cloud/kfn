-- TROLL FEATURES (The Best Ones!)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

-- HUMP SELECTED (Infinite Yield Style!)
createButton(trollPage, "Hump Selected Player (IY Style)", "ğŸ˜ˆ", 1, function(s, st)
    states.hump = not states.hump
    
    if states.hump then
        if not selectedPlayer then
            states.hump = false
            notify("âš ï¸", "Select a player first!", 2)
            return
        end
        
        -- Set character to R6-like state for humping
        if character:FindFirstChild("Right Arm") or character:FindFirstChild("RightUpperArm") then
            connections.hump = RunService.Heartbeat:Connect(function()
                if not states.hump or not selectedPlayer then return end
                
                if selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    local targetRoot = selectedPlayer.Character.HumanoidRootPart
                    
                    -- Move back and forth
                    local time = tick()
                    local offset = math.sin(time * 5) * 1.5
                    
                    -- Position behind target
                    rootPart.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 2 + offset)
                    
                    -- Face the target
                    local lookVector = (targetRoot.Position - rootPart.Position).Unit
                    rootPart.CFrame = CFrame.new(rootPart.Position, rootPart.Position + lookVector)
                    
                    -- Raise one arm (IY style)
                    if character:FindFirstChild("Right Shoulder") or character:FindFirstChild("RightShoulder") then
                        local rightShoulder = character:FindFirstChild("Right Shoulder") or character:FindFirstChild("RightShoulder")
                        rightShoulder.C0 = CFrame.new(1, 0.5, 0) * CFrame.Angles(0, 0, math.rad(90))
                    end
                end
            end)
            
            st.Text = "ON"
            s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            st.TextColor3 = Color3.fromRGB(255, 255, 255)
            notify("ğŸ˜ˆ", "Humping " .. selectedPlayer.Name, 2)
        end
    else
        if connections.hump then connections.hump:Disconnect() end
        
        -- Reset arm
        if character:FindFirstChild("Right Shoulder") or character:FindFirstChild("RightShoulder") then
            local rightShoulder = character:FindFirstChild("Right Shoulder") or character:FindFirstChild("RightShoulder")
            rightShoulder.C0 = CFrame.new(1, 0.5, 0, 0, 0, 1, 0, 1, 0, -1, 0, 0)
        end
        
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- FOLLOW SELECTED
createButton(trollPage, "Follow Selected Player", "ğŸ¯", 2, function(s, st)
    states.follow = not states.follow
    
    if states.follow then
        if not selectedPlayer then
            states.follow = false
            notify("âš ï¸", "Select a player first!", 2)
            return
        end
        
        connections.follow = RunService.Heartbeat:Connect(function()
            if not states.follow or not selectedPlayer then return end
            
            if selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local targetRoot = selectedPlayer.Character.HumanoidRootPart
                rootPart.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 3)
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
        notify("ğŸ¯", "Following " .. selectedPlayer.Name, 2)
    else
        if connections.follow then connections.follow:Disconnect() end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- ORBIT PLAYER
createButton(trollPage, "Orbit Selected Player", "ğŸŒ", 3, function(s, st)
    states.orbit = not states.orbit
    
    if states.orbit then
        if not selectedPlayer then
            states.orbit = false
            notify("âš ï¸", "Select a player first!", 2)
            return
        end
        
        local angle = 0
        connections.orbit = RunService.Heartbeat:Connect(function()
            if not states.orbit or not selectedPlayer then return end
            
            if selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local targetRoot = selectedPlayer.Character.HumanoidRootPart
                
                angle = angle + 0.1
                local x = math.cos(angle) * 8
                local z = math.sin(angle) * 8
                
                rootPart.CFrame = targetRoot.CFrame * CFrame.new(x, 2, z)
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
        notify("ğŸŒ", "Orbiting " .. selectedPlayer.Name, 2)
    else
        if connections.orbit then connections.orbit:Disconnect() end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- TP TO SELECTED
createButton(trollPage, "TP to Selected Player", "ğŸ“", 4, function(s, st)
    if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local targetRoot = selectedPlayer.Character.HumanoidRootPart
        rootPart.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 3)
        notify("âœ…", "Teleported to " .. selectedPlayer.Name, 2)
    else
        notify("âš ï¸", "Select a player first!", 2)
    end
end)

-- Entrance Animations
mainFrame.Size = UDim2.new(0, 0, 0, 0)
playerListFrame.Size = UDim2.new(0, 0, 0, 0)

wait(0.1)

TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {
    Size = UDim2.new(0, 550, 0, 550)
}):Play()

wait(0.2)

TweenService:Create(playerListFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {
    Size = UDim2.new(0, 250, 0, 550)
}):Play()

wait(1)
notify("ğŸ’€ CHAOS MODE", "LOADED! Let's cause mayhem!", 3)
wait(1)
