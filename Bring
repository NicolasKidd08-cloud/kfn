-- ANIMATION FEATURES
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

-- T-POSE
createButton(animPage, "T-Pose (Permanent T-Pose)", "ğŸ¦…", 1, function(s, st)
    states.tpose = not states.tpose
    
    if states.tpose then
        local animator = humanoid:FindFirstChildOfClass("Animator")
        if animator then
            for _, track in pairs(animator:GetPlayingAnimationTracks()) do
                track:Stop()
            end
        end
        
        if character:FindFirstChild("Left Shoulder") or character:FindFirstChild("LeftShoulder") then
            local leftShoulder = character:FindFirstChild("Left Shoulder") or character:FindFirstChild("LeftShoulder")
            leftShoulder.C0 = CFrame.new(-1.5, 0.5, 0)
        end
        if character:FindFirstChild("Right Shoulder") or character:FindFirstChild("RightShoulder") then
            local rightShoulder = character:FindFirstChild("Right Shoulder") or character:FindFirstChild("RightShoulder")
            rightShoulder.C0 = CFrame.new(1.5, 0.5, 0)
        end
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if character:FindFirstChild("Left Shoulder") or character:FindFirstChild("LeftShoulder") then
            local leftShoulder = character:FindFirstChild("Left Shoulder") or character:FindFirstChild("LeftShoulder")
            leftShoulder.C0 = CFrame.new(-1, 0.5, 0, 0, 0, -1, 0, 1, 0, 1, 0, 0)
        end
        if character:FindFirstChild("Right Shoulder") or character:FindFirstChild("RightShoulder") then
            local rightShoulder = character:FindFirstChild("Right Shoulder") or character:FindFirstChild("RightShoulder")
            rightShoulder.C0 = CFrame.new(1, 0.5, 0, 0, 0, 1, 0, 1, 0, -1, 0, 0)
        end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

