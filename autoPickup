CreateToggle("Player ESP", function(enabled)
    Config.PlayerESPEnabled = enabled
    
    if enabled then
        local function createESP(player)
            if player == LocalPlayer then return end
            
            local function addESPToChar(char)
                task.wait(0.5)
                
                local hrp = char:FindFirstChild("HumanoidRootPart")
                if not hrp then return end
                
                -- Highlight
                local highlight = Instance.new("Highlight")
                highlight.Name = "PlayerESP"
                highlight.Adornee = char
                highlight.FillColor = Color3.fromRGB(255, 100, 255)
                highlight.OutlineColor = Color3.fromRGB(255, 150, 255)
                highlight.FillTransparency = 0.5
                highlight.OutlineTransparency = 0
                highlight.Parent = char
                
                -- Billboard
                local billboard = Instance.new("BillboardGui")
                billboard.Name = "PlayerESPLabel"
                billboard.Adornee = hrp
                billboard.Size = UDim2.new(0, 200, 0, 50)
                billboard.StudsOffset = Vector3.new(0, 3, 0)
                billboard.AlwaysOnTop = true
                billboard.Parent = char
                
                local label = Instance.new("TextLabel")
                label.Size = UDim2.new(1, 0, 1, 0)
                label.BackgroundTransparency = 1
                label.Text = player.Name
                label.TextColor3 = Color3.fromRGB(255, 255, 255)
                label.TextSize = 18
                label.Font = Enum.Font.GothamBold
                label.TextStrokeTransparency = 0
                label.Parent = billboard
            end
            
            if player.Character then
                addESPToChar(player.Character)
            end
            
            player.CharacterAdded:Connect(function(char)
                if Config.PlayerESPEnabled then
                    addESPToChar(char)
                end
            end)
        end
        
        for _, player in pairs(Players:GetPlayers()) do
            createESP(player)
        end
        
        Config.PlayerESPConnection = Players.PlayerAdded:Connect(createESP)
        
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Player ESP";
            Text = "Now tracking all players!";
            Duration = 3;
        })
    else
        -- Remove all ESP
        for _, player in pairs(Players:GetPlayers()) do
            if player.Character then
                local esp = player.Character:FindFirstChild("PlayerESP")
                if esp then esp:Destroy() end
                
                local label = player.Character:FindFirstChild("PlayerESPLabel")
                if label then label:Destroy() end
            end
        end
        
        if Config.PlayerESPConnection then
            Config.PlayerESPConnection:Disconnect()
            Config.PlayerESPConnection = nil
        end
    end
end)

