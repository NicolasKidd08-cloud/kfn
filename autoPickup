-- AUTO-START FEATURES ON LOAD
-- ============================================================

task.spawn(function()
    task.wait(1) -- Wait for everything to load
    
    -- Auto-enable Desync V3 on start
    EnableDesync()
    
    -- Auto-enable Walk Speed
    Config.WalkSpeedEnabled = true
    local char = LocalPlayer.Character
    if char then
        local hum = char:FindFirstChildOfClass("Humanoid")
        if hum then
            hum.WalkSpeed = 50
        end
    end
    
    Config.WalkSpeedConnection = LocalPlayer.CharacterAdded:Connect(function()
        task.wait(0.5)
        if Config.WalkSpeedEnabled then
            local c = LocalPlayer.Character
            if c then
                local h = c:FindFirstChildOfClass("Humanoid")
                if h then h.WalkSpeed = 50 end
            end
        end
    end)
    
    -- Auto-enable Jump Power
    Config.JumpPowerEnabled = true
    if char then
        local hum = char:FindFirstChildOfClass("Humanoid")
        if hum then
            hum.JumpPower = 100
        end
    end
    
    Config.JumpPowerConnection = LocalPlayer.CharacterAdded:Connect(function()
        task.wait(0.5)
        if Config.JumpPowerEnabled then
            local c = LocalPlayer.Character
            if c then
                local h = c:FindFirstChildOfClass("Humanoid")
                if h then h.JumpPower = 100 end
            end
        end
    end)
    
    -- Auto-enable Fullbright
    Config.FullbrightEnabled = true
    game:GetService("Lighting").Brightness = 2
    game:GetService("Lighting").ClockTime = 14
    game:GetService("Lighting").FogEnd = 100000
    game:GetService("Lighting").GlobalShadows = false
    game:GetService("Lighting").OutdoorAmbient = Color3.fromRGB(128, 128, 128)
    
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "‚ö° Auto-Start Features";
        Text = "Desync, Speed, Jump & Fullbright ON!";
        Duration = 4;
    })
end)
