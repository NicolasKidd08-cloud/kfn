-- CHILLI GUI NAME FINDER
-- Shows the REAL internal name of Chilli Hub

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

print("\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("ğŸ” FINDING CHILLI HUB'S REAL NAME")
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\n")

print("ğŸ“‹ ALL GUI NAMES IN PLAYERGUI:")
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\n")

for _, gui in pairs(playerGui:GetChildren()) do
    if gui:IsA("ScreenGui") then
        print("GUI:", gui.Name)
        
        -- Check if it has any Frame with "Chilli" text
        local hasChilli = false
        for _, obj in pairs(gui:GetDescendants()) do
            if obj:IsA("TextLabel") and obj.Text then
                local text = obj.Text
                if text:find("Chilli") or text:find("Premium") or text:find("Server Tools") then
                    hasChilli = true
                    print("  â­ CONTAINS TEXT:", text)
                end
            end
        end
        
        if hasChilli then
            print("  ğŸ¯ THIS IS PROBABLY CHILLI HUB!")
            print("  ğŸ¯ REAL GUI NAME:", gui.Name)
            print("")
            
            -- Now find textboxes in this GUI
            print("  ğŸ“¦ TEXTBOXES IN THIS GUI:")
            for _, obj in pairs(gui:GetDescendants()) do
                if obj:IsA("TextBox") then
                    print("    - Found:", obj.Name)
                    print("      Path:", obj:GetFullName())
                    print("      Visible:", obj.Visible)
                    print("")
                end
            end
        end
        print("")
    end
end

print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("âœ… SEARCH COMPLETE!")
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("\nğŸ’¡ Look for the line that says:")
